<UserControl x:Class="MapleStoryHelperWPF.Control.Damage.DamageControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MapleStoryHelperWPF.Control.Damage"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <ListView x:Name="lvCharacter"
                  Style="{StaticResource lvCharacterStyle}"
                  ItemsSource="{Binding CharacterList}">
            
        </ListView>
    </Grid>
</UserControl>
