<UserControl x:Class="MapleStoryHelperWPF.Control.Damage.MulungControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MapleStoryHelperWPF.Control.Damage"
             xmlns:control="clr-namespace:MapleStoryHelperWPF.Control"
             xmlns:status="clr-namespace:MapleStoryHelperWPF.Control.Status"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <status:StatusControl VerticalAlignment="Top"/>
        <Button x:Name="btnMainSkill" Margin="0,0,0,50"
                FontSize="30"
                VerticalAlignment="Bottom"
                Width="200" Height="50"
                Content="주력기 설정"
                Click="btnMainSkill_Click"/>
    </Grid>
</UserControl>
